
class Subject
	types
		public String = seq of char
	instance variables
		private name : String;
	operations
	
		public Subject: String ==> Subject
		Subject (n) ==
			(name := n; return self)
		post name = n;
		
		public GetName: () ==> String
		GetName() ==
			(return name);
		
		public SetName: String ==> ()
		SetName(n) ==
			(name := n)
			post (name = n);
		
end Subject